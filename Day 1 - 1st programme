n=int(input("enter the number of elements"))
L=[]
    #n=[18,9,1,12,13,4,30]
while n>=1:
        i=int(input("enter the elements:"))
        L.append(i)
        n=n-1
        even=0
        odd=0
for p in L:
        if p%2==0:
            even=even+p**2
        else:
            odd=odd+p**2
L=[odd,even]
print(L)
